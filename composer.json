{
    "name": "sierratecnologia/stalker",
    "description": "Stalker de Arquivos",
    "license": "MIT",
    "authors": [
        {
            "name": "Ricardo Rebello Sierra",
            "email": "contato@ricardosierra.com.br"
        }
    ],
    "require": {
        "php": "^7.3.0",
        "laravel/framework": "^6.0|^7.0|^8.0",
        "cloudflare/sdk": "*",
        "devio/pipedrive": "*",
        "fideloper/proxy": ">=4.0",
        "league/pipeline": "*",
        "sabre/xml": "*",
        "league/csv": "^9.0|^8.0|^7.1",
        "league/flysystem": "*",
        "league/flysystem-cached-adapter": "*",
        "league/flysystem-sftp": "*",
        "league/flysystem-ziparchive": "*",
        "srmklive/flysystem-dropbox-v2": "*",
        "google/photos-library": ">=1.4",
        "intervention/image": ">=2.4",
        "intervention/imagecache": ">=2.3",
        "cipemotion/medialibrary": "*",
        "spatie/laravel-medialibrary": "^7.0",
        "league/climate": ">=3.4",
        "sierratecnologia/locaravel": "dev-master",
        "raiym/instagram-php-scraper": ">=0.9.10",
        "sierratecnologia/tracking": "dev-master",
        "league/mime-type-detection": ">=1.1"
    },
    "autoload": {
        "psr-4": {
            "Stalker\\": "src/"
        }
    },
    "bin": ["bin/stalker"],
    "scripts": {
        "post-install-cmd": [],
        "post-update-cmd": [],
        "post-create-project-cmd": []
    },
    "repositories": [
        {
            "type": "path",
            "url": "../**",
            "options": {
                "symlink": true
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Stalker\\StalkerProvider"
            ]
        }
    }
}
