        "phpmd/phpmd": "*",
        "squizlabs/php_codesniffer": "*",
        "phpro/grumphp": "*",
        "phpseclib/phpseclib": "*",
        "phpstan/phpstan": "*",
        "phpunit/phpunit": "*",
        "aws/aws-sdk-php": "*",
        "predis/predis": "*",
        "sebastian/diff": "*",
        "sebastian/phpcpd": "*",
        "sierratecnologia/tracking": "dev-master",
        "phpoffice/phppresentation": "*",
        "phpoffice/phpspreadsheet": "*",
        "phpoffice/phpword": "*",
        "league/mime-type-detection": ">=1.1"
    },
    "autoload": {
        "psr-4": {
            "Artista\\": "src/"
        }
    },
    "bin": ["bin/observador"],
    "scripts": {
        "post-install-cmd": [],
        "post-update-cmd": [],
        "post-create-project-cmd": []
    },
    "repositories": [
        {
            "type": "path",
            "url": "../**",
            "options": {
                "symlink": true
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Artista\\ArtistaProvider"
            ]
        }
    }
}
